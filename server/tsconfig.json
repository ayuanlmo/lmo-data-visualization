{
  "compilerOptions" : {
    "incremental" : true,
    "tsBuildInfoFile" : "./dist/lmo",
    "declaration" : true,
    "module" : "commonjs",
    "target" : "es6",
    "moduleResolution" : "node",
    "sourceMap" : true,
    "declarationMap" : true,
    "outDir" : "dist",
    "removeComments" : true,
    "baseUrl" : ".",
    "listFiles" : true,
    "noUnusedLocals" : true,
    "diagnostics" : true,
    "listEmittedFiles" : true,
    "noUnusedParameters" : true,
    "noFallthroughCasesInSwitch" : true,
    "paths" : {
      "*" : [
        "node_modules/*"
      ]
    }
  }
}